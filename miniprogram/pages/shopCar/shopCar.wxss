.item {
  display: flex;
  align-items: center;
  border-top: 1px solid #000;
  justify-content: space-evenly;
  padding: 30rpx 0;
}
.item:last-child {
  border-bottom: 1px solid #000;
}
.item image {
  width: 200rpx;
  height: 200rpx;
}
.info {
  display: flex;
  height: 200rpx;
  width: 360rpx;
  flex-direction: column;
  justify-content: space-between;
}
.info_top {
  display: flex;
  justify-content: space-between
}

.info_bottom {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.number_btn {
  display: flex;
}
.item button{
  width: 60rpx;
  height: 44rpx;
}
.selectedNumber{
  width: 100rpx;
  text-align: center
}
.sell_price{
  color:#EE0A24
}
.allSelect {
  padding: 0 20rpx;
}
.empty {
  margin: 0 auto;
  margin-top: 260rpx;
}
.empty .icon {
  background-color: #ccc;
  border-radius: 50%;
  border: 30rpx solid#ccc;
  transform: translateX(-50%);
  position: relative;
  left: 50%;
  margin:  30rpx 0;
}
.empty text {
  display: block;
  color: rgb(95, 95, 95);
}