page {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  width: 750rpx;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 200rpx 0;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}